@{
    ViewBag.Title = "Home Page";
}

@*<script type="text/javascript" src="~/Scripts/MasterDetail.js"></script>*@

<div class="content-wrapper" style="padding: 0; margin: 0;">
    <!-- Main content -->
    <section class="content">
        <div class="row">
            <div class="col-md-12">
                <!-- general form elements -->
                <div class="box box-primary">
                    <div class="box-header with-border">
                        <h3 class="box-title">List</h3>
                        <button type="button" class="btn btn-primary" onclick="GotoPage();">Add New</button>
                    </div>
                    <!-- /.box-header -->
                    <div class="box-body">
                        <div class="table-responsive">
                            <table class="table"  id="table_data">
                                <thead>
                                    <tr>
                                        <th style="width: 10%">Date</th>
                                        <th style="width: 60%">Customer Name</th>
                                        <th style="width: 20%">Contact No</th>
                                        <th style="width: 10%">Action</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>
                    <!-- /.box-body -->
                </div>
            </div>
        </div>
        <!-- /.row -->
    </section>
</div>